{
	"name": "website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && touch ../docs/.nojekyll",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.0",
		"@smui/card": "^7.0.0-beta.0",
		"@smui/chips": "^6.1.4",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/layout-grid": "^7.0.0-beta.0",
		"@smui/list": "^6.1.4",
		"@smui/paper": "^7.0.0-beta.0",
		"@smui/textfield": "^6.1.4",
		"@smui/top-app-bar": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.80",
		"@sveltejs/adapter-static": "^1.0.0-next.43",
		"@sveltejs/kit": "^1.0.0-next.505",
		"playwright": "^1.26.1",
		"smui-theme": "^6.2.0",
		"svelte-preprocess": "^4.10.7",
		"vite": "^3.1.4"
	},
	"type": "module"
}
